        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer drupal:scaffold"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "drupal/social": "self.version",
        "drupal/socialbase": "self.version",
        "drupal/socialblue": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
