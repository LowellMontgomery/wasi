{
    "name": "goalgorilla/open_social",
    "description": "Open Social is a Drupal 10/11 distribution for building social communities and intranets.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "composer/installers": "^1.9 || ^2.0",
        "drupal/core-composer-scaffold": "^9.5 || ^10.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "npm-asset/autosize": "^4.0.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "installer-types": ["npm-asset"],
        "installer-paths": {
            "libraries/{$name}": ["type:drupal-library", "type:npm-asset"]
        }
    }
}
