{
    "name": "lowellmontgomery/wasi-wrapper",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "require": {
        "composer/installers": "^2.0",
        "drupal/core-composer-scaffold": "^10.0",
        "lowellmontgomery/walipa_drupal_social": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/LowellMontgomery/walipa_drupal_social.git"
        }
    ],
    "extra": {
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/profiles/contrib/social": ["lowellmontgomery/walipa_drupal_social"],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "html/"
            }
        }
    }
}
